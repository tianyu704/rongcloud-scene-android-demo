<resources>

    <style name="music_audio_setting_seekbar_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">15dp</item>
        <item name="android:layout_marginEnd">15dp</item>
        <item name="android:max">100</item>
        <item name="android:paddingStart">8dp</item>
        <item name="android:paddingEnd">8dp</item>
        <item name="android:progressDrawable">@drawable/bg_music_audio_volume_seekbar</item>
        <item name="android:thumb">@drawable/thumb_audio_volume_setting</item>
    </style>
    <!--控制popupwindows进出动画-->
    <style name="popup_window_anim_style">
    <item name="android:windowEnterAnimation">@anim/pop_enter_anim</item>
    <!-- 指定显示的动画xml -->
    <item name="android:windowExitAnimation">@anim/pop_exit_anim</item>
    <!-- 指定消失的动画xml -->
</style>
</resources>